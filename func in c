#include<stdio.h>
int fun()
{
    int a=10,b=20;
    printf("%d\n",a+b);
}
int main()
{
 fun(); 
 fun();
}

func passing parameters:

#include<stdio.h>
int fun(char name[])
{
    printf("%s\n",name);
}
int main()
{
    fun("kiran");
    fun("kumar");
    fun("yadav");
}

return type parameters:
#include<stdio.h>
int fun(int a)
{
    return 5+a;
}
int main(){
    printf("%d",fun(12));
    return 0;
}
